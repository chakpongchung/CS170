\begin_inset Formula $f(n)=5n\log5n$
\end_inset

 , 
\begin_inset Formula $g(n)=n\log n$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\begin{eqnarray*}
f(n) & = & 5n\log5n\\
 & = & 5n\log5+5n\log n\\
 & = & O(n\log n)\\
g(n) & = & O(n\log n)
\end{eqnarray*}

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\therefore f(n)=\Theta(g(n))$

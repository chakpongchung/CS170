\end_inset


\begin_inset Formula $f(n)=\log500n$
\end_inset

 , 
\begin_inset Formula $g(n)=\log8n$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\begin{eqnarray*}
f(n) & = & \log500n\\
 & = & \log500+\log n\\
 & = & O(\log n)\mbox{ , \ensuremath{\log n}dominates \ensuremath{\log500}}\\
g(n) & = & \log8n\\
 & = & \log8+\log n\\
 & = & O(\log n)
\end{eqnarray*}

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\therefore f(n)=\Theta(g(n))$
\end_inset


